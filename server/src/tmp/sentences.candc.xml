<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[wq]">
  <rule type="fa" cat="S[wq]">
   <lf start="0" span="1" word="What" lemma="what" pos="WP" chunk="I-NP" entity="O" cat="S[wq]/(S[dcl]\NP)" />
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="languages" lemma="language" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="China" lemma="China" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="?" lemma="?" pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[wq]">
  <rule type="fa" cat="S[wq]">
   <lf start="0" span="1" word="Who" lemma="who" pos="WP" chunk="I-NP" entity="O" cat="S[wq]/(S[dcl]\NP)" />
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
      <lf start="3" span="1" word="Watson" lemma="Watson" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="?" lemma="?" pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="bx" cat="S[wq]/S[q]">
  <lf start="0" span="1" word="How" lemma="how" pos="WRB" chunk="I-NP" entity="O" cat="S[wq]/S[q]" />
  <rule type="lex" cat="S[X]\S[X]">
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="many" lemma="many" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="dogs" lemma="dog" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="were" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="5" span="1" word="my" lemma="my" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="6" span="1" word="house" lemma="house" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="7" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="8" span="1" word="2014" lemma="2014" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="9" span="1" word="?" lemma="?" pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
